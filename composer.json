{
    "name": "justijndepover/crop-images",
    "description": "Bulk crop images to a specific format",
    "keywords": [
        "images",
        "crop",
        "php",
        "justijndepover"
    ],
    "homepage": "https://github.com/justijndepover/crop-images",
    "license": "MIT",
    "authors": [
        {
            "name": "Justijn Depover",
            "email": "justijndepover@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "^4.2|^5.0",
        "symfony/finder": "^5.1",
        "spatie/image": "^1.7",
        "symfony/filesystem": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["crop"]
}
